<p id="notice"><%= notice %></p>

<h1>Findjobs</h1>

<%= form_tag  findjobs_path ,:method =>'get' do %>
<%= text_field_tag :search ,params[:search] %>
<%= submit_tag "CLICK HERE" %>
<% end %>

<table>
  <thead>
    <tr>
      <th>Tec</th>
      <th>Exp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @findjobs.each do |findjob| %>
      <tr>
        <td><%= findjob.tec %></td>
        <td><%= findjob.exp %></td>
        <td><%= link_to 'Show', findjob %></td>
        <td><%= link_to 'Edit', edit_findjob_path(findjob) %></td>
        <td><%= link_to 'Destroy', findjob, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Findjob', new_findjob_path %>
